[
  {
    "path": "/login",
    "component": "@/layouts/UserLayout"
  },
  {
    "path": "/register",
    "component": "@/layouts/Register",
    "isAuth": true
  },
  {
    "path": "/",
    "component": "@/layouts/BasicLayout",
    "routes": [
      {
        "path": "/home",
        "name": "首页",
        "component": "@/pages/Home",
        "icon": "home"
      },
      {
        "path": "/UserManagement",
        "name": "用户管理",
        "icon": "user",
        "routes": [
          {
            "path": "/UserManagement/User",
            "name": "用户管理",
            "icon": "user",
            "component": "../pages/UserManagement/User"
          }
        ]
      },
      {
        "path": "/chargeManagement",
        "name": "收费管理",
        "icon": "appstore-o",
        "routes": [
          {
            "path": "/chargeManagement/propertyCharge",
            "name": "物业收费",
            "icon": "shopping-cart",
            "routes": [
              {
                "path": "/chargeManagement/propertyCharge/shopCharge",
                "name": "商铺综合收费",
                "icon": "shopping-cart",
                "component": "../pages/ChargeManagement/PropertyCharge/ShopCharge"
              },
              {
                "path": "/chargeManagement/propertyCharge/temporaryCharges",
                "name": "临时收费",
                "icon": "shopping-cart",
                "component": "../pages/ChargeManagement/PropertyCharge/TemporaryCharges"
              }
            ]
          }
        ]
      },
      {
        "path": "/BasicManage",
        "name": "基础数据管理",
        "icon": "link",
        "routes": [
          {
            "path": "/BasicManage/ChargeManage",
            "name": "收费项管理",
            "icon": "copy",
            "component": "../pages/BasicManage/ChargeManage"
          },
          {
            "path": "/BasicManage/BusinessManage",
            "name": "商业区管理",
            "icon": "copy",
            "component": "../pages/BasicManage/BusinessManage"
          }
        ]
      },
      {
        "path": "/Authority",
        "name": "权限管理",
        "icon": "setting-o",
        "routes": [
          {
            "path": "/Authority/Application",
            "name": "应用菜单权限管理",
            "icon": "appstore-o",
            "component": "../pages/Authority/Application"
          },
          {
            "path": "/Authority/Role",
            "name": "角色权限设置",
            "icon": "setting-o",
            "component": "../pages/Authority/Role"
          },
          {
            "path": "/Authority/User",
            "name": "用户权限管理",
            "icon": "user-add",
            "component": "../pages/Authority/User"
          }
        ]
      },
      {
        "path": "/403",
        "name": "403",
        "hideInMenu": true,
        "component": "403"
      },
      {
        "path": "/500",
        "name": "500",
        "hideInMenu": true,
        "component": "500"
      },
      {
        "path": "/404",
        "name": "404",
        "hideInMenu": true,
        "component": "404"
      },
      {
        "path": "*",
        "name": "404",
        "component": "404"
      }
    ]
  }
]
